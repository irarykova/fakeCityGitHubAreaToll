<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<config>

    <module name="global">
        <param name="randomSeed" value="4711" />
        <param name="coordinateSystem" value="EPSG:32635" />
        <param name="numberOfThreads" value="16" />
    </module>

    <module name="network">
        <param name="inputNetworkFile" value="network_horizon2021_1_1.xml" />
    </module>

    <module name="plans">
        <param name="inputPlansFile" value= "planWithAgglomerations.xml"/>
    </module>

    <module name="controler">
        <param name="outputDirectory" value="output/scenarios/" />
        <param name="firstIteration" value="0" />
        <param name="lastIteration" value="100" />
        <param name="eventsFileFormat" value="xml" />
        <param name="mobsim" value="qsim" />
        <param name="writePlansInterval" value="10" />
        <param name="writeEventsInterval" value="10" />
        <param name="overwriteFiles" value="deleteDirectoryIfExists" />
    </module>

    <module name="roadpricing" >
        <param name="tollLinksFile" value="output_toll_zsd_bridges.xml" />
    </module>

    <module name="qsim">
        <!-- "start/endTime" of MobSim (00:00:00 == take earliest activity time/ run as long as active vehicles exist) -->
        <param name="startTime" value="00:00:00" />
        <param name="endTime" value="25:00:00" />
        <param name="numberOfThreads" value="16"/>
        <param name = "snapshotperiod"	value = "00:00:00"/> <!-- 00:00:00 means NO snapshot writing -->
        <param name="storageCapacityFactor" value="0.05" />
        <param name="flowCapacityFactor" value="0.05" />
        <param name="stuckTime" value="10.0" />
        <param name="mainMode" value="car" />
    </module>
